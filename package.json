{
  "name": "uapi-json",
  "version": "0.6.1",
  "description": "Travelport Universal API",
  "main": "build/",
  "files": [
    "build/",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive",
    "cover": "babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive",
    "coveralls": "cat coverage/*/lcov.info | codecov",
    "fix": "eslint --fix src/",
    "lint": "eslint src/",
    "build": "babel --copy-files src/ -d build/",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Travelport-Ukraine/uapi-json"
  },
  "keywords": [
    "uapi",
    "node",
    "travelport"
  ],
  "author": "Mark Orel <mail.ormark@gmail.com>",
  "constributors": [
    "Dmitry Chertousov <d.chertousov@gmail.com>",
    "Artem Pylypchuk <articicejuice@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "galileo-screen": "1.0.2",
    "handlebars": "^4.0.5",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "node-errors-helpers": "^0.1.2",
    "request": "^2.60.0",
    "xml2js": "^0.4.9"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node4": "^2.1.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "isparta": "^4.0.0",
    "mocha": "^1.21.5",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6"
  }
}
